    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "deploy:staging": "npm run build && gh-pages -d client/build -b gh-pages-staging",
    "deploy:production": "npm run build && gh-pages -d client/build -b gh-pages",
    "security:scan": "npm audit && snyk test",
    "mongodb:init": "mongosh < mongodb_schema.js",
    "docker:build": "docker build -t biometric-security .",
    "docker:run": "docker-compose up -d"
